    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "MODEL.DEVICE","cpu"
            ],
            "env": {
                "DETECTRON2_DATASETS":"/workspace/data"
            }
        }
    ]
}